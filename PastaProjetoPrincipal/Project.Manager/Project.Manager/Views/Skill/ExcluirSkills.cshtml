@model Project.Manager.Models.Skill

@{
    ViewBag.Title = "ExcluirSkills";
}

<h2>Excluir Skills</h2>

<h3>Tem certeza de que deseja excluir essa skill?</h3>
<div>
    
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.TipoSkill)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TipoSkill)
        </dd>
              

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Excluir" class="btn btn-danger" /> |
            @Html.ActionLink("Voltar para a lista", "ListarSkills")
        </div>
    }
</div>
